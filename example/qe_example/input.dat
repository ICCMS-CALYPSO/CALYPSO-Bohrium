# System Name default=CALYPSO 
SystemName = As 
# the number of elements
NumberOfSpecies = 1
# the name of element (The order is the same as POTCAR)
NameOfAtoms = As 

# the number of elements
NumberOfAtoms = 2

# atomic numbure
AtomicNumber = 33  

# the max step of iteration
MaxStep =50
VSC=F
@CtrlRange
1 4
@end
# the volume of 1 f.u. unit=angstrom^3
Volume= 15

# the shortest distance among the atoms
# 11 
# 11 22 12
# 11 22 33 12 13 23
@DistanceOfIon 
1.5
@end
# the ratio of survive to do PSO
PsoRatio = 0.6

# the algorithm
Ialgo = 2
#sort_flag=T
r_control= 1 6
vdw_flag=F
ICode=  4
# the KPOINTS Distance
Kgrid = 0.12 0.08

# The command to submit the job (according to your computors)
Command = sh submit.sh

# the number of formula in the unit cell
NumberOfFormula = 1 4

# the Population size
PopSize =10

# only used in molecular crystal structure(implanted, testing now)
mol=F

MaxTime= 3600
PickUp=F
#PickStep=1
NumberOfLocalOptim = 3
# Debug=T
 #MOLdis=0.75 1.20
#Spespacegroup = 164
# only used for cluster (testing now)
#cluster= T
# special constrain
  #spepg = D3d (point group)
  #spesg = 20  (special atoms_)
  #isym= .true. (not support now)
  #constrain = .true. (not support now)
Split=T
